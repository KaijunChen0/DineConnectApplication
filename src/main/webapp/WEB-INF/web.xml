<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>UserLoginServlet</servlet-name>
        <servlet-class>dineconnect.servlet.UserLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserLoginServlet</servlet-name>
        <url-pattern>/userlogin</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BusinessLoginServlet</servlet-name>
        <servlet-class>dineconnect.servlet.BusinessLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BusinessLoginServlet</servlet-name>
        <url-pattern>/businesslogin</url-pattern>
    </servlet-mapping>



</web-app>
